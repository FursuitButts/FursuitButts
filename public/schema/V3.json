{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://yiff.rest/schema/V3.json",
	"type": "object",
	"required": [
		"success",
		"images"
	],
	"properties": {
		"success": {
			"$id": "#properties/success",
			"type": "boolean",
			"title": "Success",
			"description": "If this request was successful"
		},
		"images": {
			"$id": "#properties/images",
			"type": "array",
			"title": "Images",
			"description": "The requested images",
			"additionalItems": true,
			"items": {
				"$id": "#properties/images/items",
				"type": "object",
				"required": [
					"approver",
					"artists",
					"createdAt",
					"ext",
					"height",
					"id",
					"md5",
					"name",
					"rating",
					"reportURL",
					"score",
					"shortURL",
					"size",
					"sources",
					"tags",
					"type",
					"updatedAt",
					"uploader",
					"url",
					"width"
				],
				"properties": {
					"approver": {
						"$id": "#properties/images/items/properties/approver",
						"type": [
							"object",
							"null"
						],
						"title": "Approver",
						"description": "The user that approved the post (null if none).",
						"required": [
							"id",
							"name"
						],
						"properties": {
							"id": {
								"$id": "#properties/images/items/properties/approver/properties/id",
								"type": "integer",
								"title": "ID",
								"description": "The id of the user that approved the post."
							},
							"name": {
								"$id": "#properties/images/items/properties/approver/properties/name",
								"type": "string",
								"title": "Name",
								"description": "The username of the user that approved the post."
							}
						},
						"additionalProperties": true
					},
					"artists": {
						"$id": "#properties/images/items/properties/artists",
						"type": "array",
						"title": "Artists",
						"description": "The artists that created the image.",
						"additionalItems": true,
						"items": {
							"$id": "#properties/images/items/properties/artists/items",
							"type": "string"
						}
					},
					"createdAt": {
						"$id": "#properties/images/items/properties/createdAt",
						"type": "string",
						"title": "Created At",
						"description": "The ISO-8601 timestamp at which this post was created."
					},
					"directURL": {
						"$id": "#properties/images/items/properties/directURL",
						"type": "string",
						"title": "Direct URL",
						"description": "The direct url to the web representation of this post."
					},
					"ext": {
						"$id": "#properties/images/items/properties/ext",
						"type": "string",
						"title": "Extension",
						"description": "The extension of this image."
					},
					"height": {
						"$id": "#properties/images/items/properties/height",
						"type": "integer",
						"title": "Height",
						"description": "The verticle height of this image."
					},
					"id": {
						"$id": "#properties/images/items/properties/id",
						"type": "integer",
						"title": "ID",
						"description": "The id of the post."
					},
					"md5": {
						"$id": "#properties/images/items/properties/md5",
						"type": "string",
						"title": "MD5",
						"description": "The md5 of the image associated with this post."
					},
					"name": {
						"$id": "#properties/images/items/properties/name",
						"type": "string",
						"title": "Name",
						"description": "(same as md5, exists for legacy reasons)"
					},
					"rating": {
						"$id": "#properties/images/items/properties/rating",
						"type": "string",
						"enum": [
							"s",
							"q",
							"e"
						],
						"title": "Rating",
						"description": "The rating of the post."
					},
					"reportURL": {
						"$id": "#properties/images/items/properties/reportURL",
						"type": "string",
						"title": "Report URL",
						"description": "(same as directURL, exists for legacy reasons)"
					},
					"score": {
						"$id": "#properties/images/items/properties/score",
						"type": "object",
						"title": "Score",
						"description": "The score of the post.",
						"required": [
							"up",
							"down",
							"total"
						],
						"properties": {
							"up": {
								"$id": "#properties/images/items/properties/score/properties/up",
								"type": "integer",
								"title": "Upvotes",
								"description": "The upvotes on this post."
							},
							"down": {
								"$id": "#properties/images/items/properties/score/properties/down",
								"type": "integer",
								"title": "Downvotes",
								"description": "The downvotes on this post."
							},
							"total": {
								"$id": "#properties/images/items/properties/score/properties/total",
								"type": "integer",
								"title": "Total",
								"description": "The total score of this post"
							}
						},
						"additionalProperties": true
					},
					"shortURL": {
						"$id": "#properties/images/items/properties/shortURL",
						"type": "string",
						"title": "Short URL",
						"description": "(same as directURL, exists for legacy reasons)"
					},
					"size": {
						"$id": "#properties/images/items/properties/size",
						"type": "integer",
						"title": "Size",
						"description": "The size in bytes of this image."
					},
					"sources": {
						"$id": "#properties/images/items/properties/sources",
						"type": "array",
						"title": "Sources",
						"description": "The sources for this post.",
						"additionalItems": true,
						"items": {
							"$id": "#properties/images/items/properties/sources/items",
							"type": "string"
						}
					},
					"tags": {
						"$id": "#properties/images/items/properties/tags",
						"type": "object",
						"title": "Tags",
						"description": "The tags for this post.",
						"required": [
							"general",
							"species",
							"character",
							"copyright",
							"artist",
							"invalid",
							"lore",
							"meta"
						],
						"properties": {
							"general": {
								"$id": "#properties/images/items/properties/tags/properties/general",
								"type": "array",
								"title": "General Tags",
								"additionalItems": true,
								"items": {
									"$id": "#properties/images/items/properties/tags/properties/general/items",
									"type": "string"
								}
							},
							"species": {
								"$id": "#properties/images/items/properties/tags/properties/species",
								"type": "array",
								"title": "Species Tags",
								"additionalItems": true,
								"items": {
									"$id": "#properties/images/items/properties/tags/properties/species/items",
									"type": "string"
								}
							},
							"character": {
								"$id": "#properties/images/items/properties/tags/properties/character",
								"type": "array",
								"title": "Character Tags",
								"additionalItems": true,
								"items": {
									"$id": "#properties/images/items/properties/tags/properties/character/items",
									"type": "string"
								}
							},
							"copyright": {
								"$id": "#properties/images/items/properties/tags/properties/copyright",
								"type": "array",
								"title": "Copyright Tags",
								"additionalItems": true,
								"items": {
									"$id": "#properties/images/items/properties/tags/properties/copyright/items",
									"type": "string"
								}
							},
							"artist": {
								"$id": "#properties/images/items/properties/tags/properties/artist",
								"type": "array",
								"title": "Artist Tags",
								"additionalItems": true,
								"items": {
									"$id": "#properties/images/items/properties/tags/properties/artist/items",
									"type": "string"
								}
							},
							"invalid": {
								"$id": "#properties/images/items/properties/tags/properties/invalid",
								"type": "array",
								"title": "Invalid Tags",
								"additionalItems": true,
								"items": {
									"$id": "#properties/images/items/properties/tags/properties/invalid/items",
									"type": "string"
								}
							},
							"lore": {
								"$id": "#properties/images/items/properties/tags/properties/lore",
								"type": "array",
								"title": "Lore Tags",
								"additionalItems": true,
								"items": {
									"$id": "#properties/images/items/properties/tags/properties/lore/items",
									"type": "string"
								}
							},
							"meta": {
								"$id": "#properties/images/items/properties/tags/properties/meta",
								"type": "array",
								"title": "Meta Tags",
								"additionalItems": true,
								"items": {
									"$id": "#properties/images/items/properties/tags/properties/meta/items",
									"type": "string",
									"items": {
										"$id": "#properties/images/items/properties/tags/properties/meta/items",
										"type": "string"
									}
								}
							}
						},
						"additionalProperties": true
					},
					"type": {
						"$id": "#properties/images/items/properties/type",
						"type": "string",
						"title": "Mime Type",
						"description": "The mime type of this image."
					},
					"updatedAt": {
						"$id": "#properties/images/items/properties/updatedAt",
						"type": "string",
						"title": "Updated At",
						"description": "The ISO-8601 timestamp at which this post was last updated."
					},
					"uploader": {
						"$id": "#properties/images/items/properties/uploader",
						"type": "object",
						"title": "Uploader",
						"description": "The uploader of this post.",
						"required": [
							"id",
							"name"
						],
						"properties": {
							"id": {
								"$id": "#properties/images/items/properties/uploader/properties/id",
								"type": "integer",
								"title": "ID",
								"description": "The id of the user that uploaded the post."
							},
							"name": {
								"$id": "#properties/images/items/properties/uploader/properties/name",
								"type": "string",
								"title": "Name",
								"description": "The username of the user that uploaded the post."
							}
						},
						"additionalProperties": true
					},
					"url": {
						"$id": "#properties/images/items/properties/url",
						"type": "string",
						"title": "File URL",
						"description": "The url of the file for this image."
					},
					"width": {
						"$id": "#properties/images/items/properties/width",
						"type": "integer",
						"title": "Width",
						"description": "The horizontal width of this image."
					}
				},
				"additionalProperties": true
			}
		}
	},
	"additionalProperties": true
}
